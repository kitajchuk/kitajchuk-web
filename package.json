{
  "name": "kitajchuk-web",
  "version": "0.1.0",
  "description": "Once a day unlearn something new.",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint components pages lib --ext js",
    "predev": "yarn ipfs",
    "dev": "next",
    "start": "next start",
    "prebuild": "yarn lint",
    "build": "next build",
    "export": "next export",
    "upload": "env-cmd -f .env.local yarn upload-s3",
    "upload-s3": "aws s3 sync ./public/img s3://${KITA_S3_REPOSITORY}/public/img --exclude '*.DS_Store' --delete",
    "postupload-s3": "aws s3 sync ./public/media s3://${KITA_S3_REPOSITORY}/public/media --exclude '*.DS_Store' --delete",
    "download": "env-cmd -f .env.local yarn download-s3",
    "download-s3": "node ./lib/aws.js",
    "ipfs": "node ./lib/ipfs.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "nanoid": "^3.1.22",
    "next": "^10.2.3",
    "preact": "^10.5.13",
    "properjs-blit": "^0.3.2",
    "properjs-imageloader": "^0.4.0",
    "properjs-scrollcontroller": "^0.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.919.0",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "image-size": "^1.0.0",
    "ipfs": "^0.60.0",
    "ipfs-core": "^0.12.1",
    "mkdirp": "^1.0.4",
    "sass": "^1.32.8"
  }
}
