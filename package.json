{
  "name": "kitajchuk-web",
  "version": "0.1.0",
  "description": "Once a day unlearn something new.",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint components pages lib --ext js",
    "dev": "next",
    "start": "next start",
    "prebuild": "yarn lint",
    "build": "next build",
    "export": "next export",
    "upload": "env-cmd -f .env.production.local npm run upload-s3",
    "upload-s3": "aws s3 sync ./public/img s3://${KITA_S3_REPOSITORY}/public/img --exclude '*.DS_Store'",
    "postupload-s3": "aws s3 sync ./public/media s3://${KITA_S3_REPOSITORY}/public/media --exclude '*.DS_Store'",
    "download": "env-cmd -f .env.production.local npm run download-s3",
    "download-s3": "node ./lib/aws.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "nanoid": "^3.1.22",
    "next": "^10.2.3",
    "properjs-blit": "^0.3.2",
    "properjs-imageloader": "^0.4.0",
    "properjs-scrollcontroller": "^0.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.919.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "image-size": "^1.0.0",
    "mkdirp": "^1.0.4",
    "sass": "^1.32.8"
  }
}
